<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.my.metadata.model.metadata" table="metadata" catalog="my">
        <id name="metadataId" type="java.lang.Long">
            <column name="METADATA_ID" />
            <generator class="identity" />
        </id>
        <property name="metadataCode" type="string">
            <column name="METADATA_CODE" length="10" not-null="true" unique="true" />
        </property>
        <property name="metadataName" type="string">
            <column name="METADATA_NAME" length="20" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>